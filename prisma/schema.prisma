model tbl_notebook_bestellung {
  ID              Int         @id @unique(map: "tblnotebookbestellung_ID_uindex")
  ProjektID       Int
  NotebookID      Int
  Beauftragt_von  String?     @db.VarChar(50)
  BestellungDatum DateTime?   @default(now()) @db.DateTime(0)
  Menga           Int?        @default(1)
  tblnotebook     tblnotebook @relation(fields: [NotebookID], references: [NotebookID], onDelete: NoAction, onUpdate: NoAction, map: "tbl_notebook_bestellung_tblnotebook_NotebookID_fk")
  tblprojekte     tblprojekte @relation(fields: [ProjektID], references: [ID], onDelete: NoAction, onUpdate: NoAction, map: "tbl_notebook_bestellung_tblprojekte_ID_fk")

  @@index([NotebookID], map: "tbl_notebook_bestellung_tblnotebook_NotebookID_fk")
  @@index([ProjektID], map: "tbl_notebook_bestellung_tblprojekte_ID_fk")
}

